package br.com.exerciciosjava.iniciante;

public class ExercicioIniciante04 {
	
	public static void main(String[] args) {
		//variables
		double LarguraParede, AlturaParede, AlturaAzulejo, LarguraAzulejo, qtde;
		
		LarguraParede = 2.6;
		AlturaParede = 1.6;
		AlturaAzulejo = 0.35;
		LarguraAzulejo = 0.3;
		
		qtde = (LarguraParede * AlturaParede) / (AlturaAzulejo * LarguraAzulejo);
		
		System.out.println("Quantidade de Azulejos Necess√°ria para essa parede: " + qtde);
	}
}
